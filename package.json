{
  "name": "capetown-game",
  "version": "1.0.0",
  "description": "The game Pac-Man",
  "main": "server/index.ts",
  "scripts": {
    "clean": "rimraf dist",
    "start:webpack": "cross-env NODE_ENV=development webpack --mode=development --watch",
    "start:server": "wait-on dist/server.js && node dist/server.js --watch",
    "start": "npm run clean && npm-run-all --print-label --parallel start:*",
    "build": "npm run clean  && cross-env NODE_ENV=production && webpack --mode production",
    "build:serve": "node -r esm dist/server.bundle.js",
    "lint:style": "stylelint --fix ./src/**/*.scss",
    "lint:ts": "eslint . --ext .tsx,.ts,.js",
    "test": "jest --runInBand --config .jest/config.json",
    "pre-commit": "npm-run-all lint:* ",
    "pre-push": "npm-run-all lint:*"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit",
      "pre-push": "npm run pre-push"
    }
  },
  "repository": {
